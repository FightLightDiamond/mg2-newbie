<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">casio_physicalstore_physicalstore_form.physicalstore_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Casio\PhysicalStore\Block\Adminhtml\PhysicalStore\Edit\BackButton" name="back"/>
			<button class="Casio\PhysicalStore\Block\Adminhtml\PhysicalStore\Edit\DeleteButton" name="delete"/>
			<button class="Casio\PhysicalStore\Block\Adminhtml\PhysicalStore\Edit\SaveButton" name="save"/>
			<button class="Casio\PhysicalStore\Block\Adminhtml\PhysicalStore\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>casio_physicalstore_physicalstore_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>casio_physicalstore_physicalstore_form.physicalstore_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="physicalstore_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Casio\PhysicalStore\Model\PhysicalStore\DataProvider" name="physicalstore_form_data_source">
			<settings>
				<requestFieldName>physicalstore_id</requestFieldName>
				<primaryFieldName>physicalstore_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field formElement="textarea" name="name" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">PhysicalStore</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="code" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">PhysicalStore</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">code</label>
				<dataScope>code</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="opening_time" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">PhysicalStore</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">opening_time</label>
				<dataScope>opening_time</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="phone_number" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">PhysicalStore</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">phone_number</label>
				<dataScope>phone_number</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="regular_holiday" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">PhysicalStore</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">regular_holiday</label>
				<dataScope>regular_holiday</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
